{
  "name": "lotus-island",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "prisma generate && next build --turbopack",
    "start": "next start",
    "lint": "eslint",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:migrate": "prisma migrate dev",
    "db:migrate:deploy": "prisma migrate deploy",
    "db:studio": "prisma studio",
    "db:seed": "dotenv -e .env -- tsx prisma/seed.ts",
    "db:init": "node database/execute-sql.js init.sql",
    "db:reset": "node database/execute-sql.js reset.sql",
    "db:check": "dotenv -e .env -- node database/check-data.js",
    "db:deploy": "npm run db:migrate:deploy && npm run db:seed",
    "deploy": "dotenv -e .env -- node scripts/deploy.js",
    "verify": "node scripts/verify-deployment.js",
    "vercel-build": "prisma generate && prisma migrate deploy && npm run build",
    "postinstall": "prisma generate"
  },
  "dependencies": {
    "@prisma/client": "^6.14.0",
    "@types/jsonwebtoken": "^9.0.10",
    "bcryptjs": "^3.0.2",
    "jsonwebtoken": "^9.0.2",
    "next": "15.5.0",
    "prisma": "^6.14.0",
    "react": "19.1.0",
    "react-dom": "19.1.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4",
    "@types/bcryptjs": "^3.0.0",
    "@types/node": "^20",
    "@types/pg": "^8.15.5",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "dotenv-cli": "^10.0.0",
    "eslint": "^9",
    "eslint-config-next": "15.5.0",
    "pg": "^8.16.3",
    "tailwindcss": "^4",
    "tsx": "^4.20.4",
    "typescript": "^5"
  }
}